config geolocate 'main'
	option enabled '1'
	option interval '300'
	option log_level 'warning'

config backend 'backend'
	option type 'beacondb'
	option google_api_key ''
	option unwiredlabs_api_key ''
	option custom_api_key ''
	option custom_url ''

config scanning 'scanning'
	option wifi_enabled '1'
	option wifi_radio 'wlan0'
	option wifi_min_aps '2'
	option cell_enabled '1'
	option cell_method 'auto'
	option nomap_filter '1'

config output 'output_traccar'
	option type 'traccar'
	option enabled '0'
	option url ''
	option device_id ''

config output 'output_owntracks'
	option type 'owntracks'
	option enabled '0'
	option url ''
	option username ''
	option password ''
	option device ''
	option tid ''

config output 'output_ha'
	option type 'homeassistant'
	option enabled '0'
	option ha_url ''
	option access_token ''
	option webhook_id ''

config output 'output_dawarich'
	option type 'dawarich'
	option enabled '0'
	option url ''
	option api_key ''

config output 'output_webhook'
	option type 'webhook'
	option enabled '0'
	option url ''
	option method 'POST'
	option device_id ''
	option header_name ''
	option header_value ''
